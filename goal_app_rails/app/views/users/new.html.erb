<h1>Sign Up</h1>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>

  <label for="Username">
    <input type="text" name="user[username]" id="Username">
  </label>

  <label for="Password">
    <input type="text" name="user[password]" id="Password">
  </label>

  <input type="submit" value="Sign Up">
</form>
